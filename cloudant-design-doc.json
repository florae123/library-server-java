{
  "_id": "_design/BookIdx",
  "views": {
    "tagView": {
      "map": "function (doc) {\r\n  var i;\r\n  for(i in doc.tags)\r\n     emit(doc.tags[i], doc._id);\r\n}"
    }
  },
  "language": "javascript",
  "indexes": {
    "titleSearch": {
      "analyzer": "standard",
      "index": "function (doc) {\n  index(\"default\", doc.title);\n}"
    }
  }
}
